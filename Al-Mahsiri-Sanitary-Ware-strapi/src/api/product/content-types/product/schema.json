{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "quantityInStock": {
      "type": "integer",
      "required": true
    },
    "image": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "discount": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::discount.discount"
    },
    "order_products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-product.order-product",
      "mappedBy": "product"
    }
  }
}
